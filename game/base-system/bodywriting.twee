			<<if not $skin[_bodypart].pen>>
				<<break>>
			<</if>>
		<</if>>
	<<case 8>>
		<<if ($worn.lower.exposed gte 2 or $worn.lower.anus_exposed gte 1) and ($worn.under_lower.exposed gte 1 or !$worn.under_lower.type.includes("athletic"))>>
